{
	"extends": "af-tsconfig",
	"compilerOptions": {
		"baseUrl": ".",
		"module": "es2022",
		"target": "es2022",
		"paths": {
			"@lib": ["../../astro-i18n/lib"],
			"@lib/*": ["../../astro-i18n/lib/*"],
			"@src": ["../../astro-i18n/src", "./src"],
			"@src/*": ["../../astro-i18n/src/*", "./src/*"]
		},
		"jsx": "react-jsx",
		"jsxImportSource": "react"
	},
	"exclude": ["dist"]
}
