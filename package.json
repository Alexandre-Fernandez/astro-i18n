{
	"name": "astro-i18n-monorepo",
	"private": true,
	"scripts": {
		"dev": "turbo run dev",
		"test": "turbo run test",
		"build": "turbo run build",
		"publish:patch": "turbo run publish:patch --filter=astro-i18n",
		"publish:minor": "turbo run publish:minor --filter=astro-i18n",
		"publish:major": "turbo run publish:major --filter=astro-i18n",
		"lint": "turbo run lint",
		"format": "prettier --write \"**/*.{ts,tsx,md}\""
	},
	"devDependencies": {
		"af-prettierrc": "latest",
		"eslint": "^7.32.0",
		"eslint-config-af-typescript": "^1.6.1",
		"prettier": "^2.8.8",
		"turbo": "^1.10.13",
		"typescript": "^5.2.2"
	},
	"packageManager": "pnpm@7.15.0",
	"engines": {
		"node": ">=18.0.0"
	}
}
